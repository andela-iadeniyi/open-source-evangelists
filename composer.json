{
    "name": "ibonly/github-status-evangelists",
    "description": "A package that grade a github user based on the user's contribution on open source project.",
    "license": "MIT",
    "authors": [
        {
            "name": "Adeniyi Ibraheem",
            "email": "ibonly01@gmail.com"
        }
    ],
    "autoload": {
        "classmap": ["src/"],
        "psr-4"   : {
            "Ibonly\\GithubStatusEvangelist\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["test/"],
        "psr-4"   : {
            "Ibonly\\GithubStatusEvangelist\\Test\\": "test/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "vlucas/phpdotenv": "^2.0",
        "guzzlehttp/guzzle": "4.0",
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    }
}
